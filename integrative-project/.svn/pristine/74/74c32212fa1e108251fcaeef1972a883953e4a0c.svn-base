.scroll-bar
{
    -fx-background-color: rgba(23, 79, 115, 0.8);
    -fx-effect: innershadow(three-pass-box, rgb(0, 30, 44), 5, 0.7, 0, 0);
    -fx-border-color: rgb(0, 30, 44);
}

.scroll-bar .increment-button
{
    -fx-background-color: rgba(23, 79, 115, 0.8);
    -fx-stroke: rgb(0, 30, 44);
}

.scroll-bar .decrement-button
{
    -fx-background-color: rgba(23, 79, 115, 0.8);
    -fx-stroke: rgb(0, 30, 44);
}

.scroll-bar .thumb
{
    -fx-background-color: rgb(23, 79, 115);
    -fx-effect: innershadow(three-pass-box, rgb(0, 30, 44), 5, 0.7, 0, 0);
}

.scroll-bar .thumb:hover
{
    -fx-effect: dropshadow(gaussian, white, 10, 0.6, 0, 0);
}

.table-cell 
{
    -fx-background-color: rgba(23, 79, 115, 0.5);
    -fx-border-color: rgb(23, 79, 115) rgb(23, 79, 115) rgb(23, 79, 115) rgb(23, 79, 115); //rgb(0, 30, 44);
    -fx-text-fill: white;
    -fx-alignment: center;
    
}

.table-cell .text
{
    -fx-font-family: "Control Freak";
    -fx-font-weight: bold;
    -fx-font-size: 20;
}

.table-row-cell:filled:focused:selected
{
    -fx-background-color: transparent;
}

.column-header
{
    -fx-effect: innershadow(three-pass-box, rgb(0, 30, 44), 5, 0.7, 0, 0);
    -fx-background-color: rgba(23, 79, 115, 0.8);
    -fx-border-color: rgb(0, 30, 44);
}

.column-header .text
{
    -fx-font-family: "Venus Rising";
    -fx-font-size: 12;
    -fx-fill: rgb(0, 26, 38);
    -fx-effect: dropshadow(gaussian, white, 10, 0.6, 0, 0);

}

.column-header-background .filler 
{
  -fx-background-color: rgba(1, 11, 12, 1);
  -fx-padding: 1em;
}